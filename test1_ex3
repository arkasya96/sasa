print n
for i in x:
    try:
        i = float(i)
    except Exception as e:
        print e.Value
    if i > 2 and i < 5:
        print 'true\n'
    else:
        print 'false\n'

suma = 0
for i in x:
    try:
        i = float(i)
    except Exception as e:
        print e.Value
    suma += i

medieAritm = suma/len(x)
